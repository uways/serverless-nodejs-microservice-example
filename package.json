{
  "name": "serverless-nodejs-microservice-example",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "offline": "serverless offline --stage offline",
    "db:install": "serverless dynamodb install",
    "db:start": "serverless dynamodb start",
    "start": "concurrently --kill-others \"npm:db:start\" \"npm:build && npm run offline\"",
    "deploy": "npm run build && serverless deploy"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.8",
    "@nestjs/core": "^9.0.5",
    "@nestjs/platform-express": "^6.11.8",
    "aws-lambda": "^1.0.5",
    "aws-sdk": "^2.624.0",
    "aws-serverless-express": "^3.3.6",
    "develop": "^1.0.0",
    "dynamoose": "^1.11.1",
    "express": "^4.17.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "source-map-support": "^0.5.10",
    "uuid": "^3.4.0",
    "xstate": "^4.7.8"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.11.8",
    "@types/aws-lambda": "^8.10.17",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/jest": "^25.1.3",
    "@types/node": "^10.17.16",
    "@types/uuid": "^3.4.7",
    "concurrently": "^5.1.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "serverless": "^1.64.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^5.12.1",
    "serverless-plugin-optimize": "^4.1.4-rc.1",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.8.2",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uways/serverless-nodejs-microservice-example.git"
  }
}
